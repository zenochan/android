<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:paddingBottom="12dp"
  android:paddingLeft="56dp"
  android:paddingRight="56dp"
  android:paddingTop="12dp"
  >

  <item>
    <shape>
      <solid android:color="@color/md.grey200"/>
    </shape>
  </item>

  <item
    android:left="-1dp"
    android:right="-1dp"
    android:top="-1dp"
    >
    <shape>
      <stroke
        android:width="1dp"
        android:color="@color/md.grey300"/>
    </shape>

  </item>

  <item
    android:bottom="8dp"
    android:left="48dp"
    android:right="48dp"
    android:top="8dp"
    >
    <shape>
      <corners android:radius="4dp"/>
      <solid android:color="@color/z.white"/>
    </shape>
  </item>


</layer-list>